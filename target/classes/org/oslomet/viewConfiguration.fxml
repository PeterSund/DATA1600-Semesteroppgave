<AnchorPane prefHeight="515.0" prefWidth="767.0" style="-fx-background-color: #D2D2D2#D2D2D2 #D2D2D2#D2D2D2;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.oslomet.ViewConfigurationsController">
