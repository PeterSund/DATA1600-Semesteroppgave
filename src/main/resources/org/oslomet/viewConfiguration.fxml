<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.control.cell.PropertyValueFactory?>
<AnchorPane prefHeight="515.0" prefWidth="767.0" style="-fx-background-color: #D2D2D2#D2D2D2 #D2D2D2#D2D2D2;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.oslomet.ViewConfigurationsController">
   <children>
      <Button fx:id="btnEditConfig" layoutX="49.0" layoutY="383.0" mnemonicParsing="false" onAction="#changePrimaryController" text="Edit configuration" />
      <Label fx:id="lbl1" layoutX="145.0" layoutY="448.0" prefHeight="34.0" prefWidth="79.0" style="-fx-border-color: black;" />
      <Button layoutX="40.0" layoutY="452.0" mnemonicParsing="false" onAction="#getLastConf" text="Get last conf" />
      <Label layoutX="283.0" layoutY="24.0" prefHeight="34.0" prefWidth="259.0" text="My configurations">
         <font>
            <Font size="28.0" />
         </font>
      </Label>
      <MenuBar fx:id="menubarEditConfig" layoutY="-1.0" prefHeight="31.0" prefWidth="177.0" style="-fx-border-color: black;">
         <menus>
            <Menu fx:id="loginAdmin" mnemonicParsing="false" onAction="#adminLogin" text="Log in admin" />
            <Menu fx:id="getHelp" mnemonicParsing="false" onAction="#showHelp" text="Help" />
         </menus>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </MenuBar>
      <TableView fx:id="tableviewMyConfigs" layoutX="49.0" layoutY="88.0" prefHeight="291.0" prefWidth="670.0">
        <columns>

          <TableColumn fx:id="colConfigName" prefWidth="75.0" text="C1">
             <cellValueFactory>
                <PropertyValueFactory property="configName" />
             </cellValueFactory>
          </TableColumn>

          <TableColumn fx:id="colTotalPrice" prefWidth="75.0" text="C2">
             <cellValueFactory>
                <PropertyValueFactory property="totalPrice" />
             </cellValueFactory>
          </TableColumn>

           <TableColumn fx:id="colTotalPerformanceValue" prefWidth="75.0" text="C2">
              <cellValueFactory>
                 <PropertyValueFactory property="totalPerformanceValue" />
              </cellValueFactory>
           </TableColumn>

           <TableColumn fx:id="colCPU" prefWidth="75.0" text="C2">
              <cellValueFactory>
                 <PropertyValueFactory property="CPU" />
              </cellValueFactory>
           </TableColumn>

           <TableColumn fx:id="colGPU" prefWidth="75.0" text="C2">
              <cellValueFactory>
                 <PropertyValueFactory property="GPU" />
              </cellValueFactory>
           </TableColumn>

           <TableColumn fx:id="colRAM" prefWidth="75.0" text="C2">
              <cellValueFactory>
                 <PropertyValueFactory property="RAM" />
              </cellValueFactory>
           </TableColumn>

           <TableColumn fx:id="colHardDrive" prefWidth="75.0" text="C2">
              <cellValueFactory>
                 <PropertyValueFactory property="hardDrive" />
              </cellValueFactory>
           </TableColumn>

           <TableColumn fx:id="colMotherboard" prefWidth="75.0" text="C2">
              <cellValueFactory>
                 <PropertyValueFactory property="motherboard" />
              </cellValueFactory>
           </TableColumn>

           <TableColumn fx:id="colPSU" prefWidth="75.0" text="C2">
              <cellValueFactory>
                 <PropertyValueFactory property="PSU" />
              </cellValueFactory>
           </TableColumn>

           <TableColumn fx:id="colSoundCard" prefWidth="75.0" text="C2">
              <cellValueFactory>
                 <PropertyValueFactory property="soundCard" />
              </cellValueFactory>
           </TableColumn>

           <TableColumn fx:id="colComputerCase" prefWidth="75.0" text="C2">
              <cellValueFactory>
                 <PropertyValueFactory property="computerCase" />
              </cellValueFactory>
           </TableColumn>

           <TableColumn fx:id="colKeyboard" prefWidth="75.0" text="C2">
              <cellValueFactory>
                 <PropertyValueFactory property="keyboard" />
              </cellValueFactory>
           </TableColumn>

           <TableColumn fx:id="colMonitor" prefWidth="75.0" text="C2">
              <cellValueFactory>
                 <PropertyValueFactory property="monitor" />
              </cellValueFactory>
           </TableColumn>

           <TableColumn fx:id="colMouse" prefWidth="75.0" text="C2">
              <cellValueFactory>
                 <PropertyValueFactory property="mouse" />
              </cellValueFactory>
           </TableColumn>

        </columns>
      </TableView>
      <Button fx:id="btnDeleteConfig" layoutX="185.0" layoutY="383.0" mnemonicParsing="false" onAction="#deleteConfig" text="Delete configuration" />
      <Button fx:id="btnSaveConfigs" layoutX="535.0" layoutY="435.0" mnemonicParsing="false" onAction="#saveConfigs" text="Save configurations">
         <font>
            <Font size="17.0" />
         </font>
      </Button>
      <Button layoutX="224.0" layoutY="452.0" mnemonicParsing="false" onAction="#changePrimaryController" text="change" />
   </children>
</AnchorPane>
